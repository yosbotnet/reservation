{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-modules-commonjs", {
      "strict": false,
      "allowTopLevelThis": true,
      "loose": true
    }],
    ["inline-dotenv",{
      "path": ".env" 
    }]
  ]
}
